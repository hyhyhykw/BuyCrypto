<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.buycrypto">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BuyCrypto">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="buy">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CoinListActivity"
            android:label="Coin list" />
        <activity android:name=".EnterAmountActivity" android:label="Enter amount"/>
        <activity android:name=".SupplierActivity" android:label="Choose Supplier"/>
        <activity android:name=".WebActivity" android:label="Payment"/>
    </application>

</manifest>